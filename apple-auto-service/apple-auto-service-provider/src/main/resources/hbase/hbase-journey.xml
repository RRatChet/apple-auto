<SimpleHbase>
    	
	<HBaseTableSchema tableName="journey" defaultFamily="info">
		<HBaseColumnSchema qualifier="account" typeName="string" />
		<HBaseColumnSchema qualifier="startTime" typeName="long" />
		<HBaseColumnSchema qualifier="endTime" typeName="long" />
		<HBaseColumnSchema qualifier="totalTime" typeName="int" />
		<HBaseColumnSchema qualifier="mileage" typeName="int" />
		<HBaseColumnSchema qualifier="oilWear" typeName="double" />
		<HBaseColumnSchema qualifier="idleTime" typeName="int" />
		<HBaseColumnSchema qualifier="rushUpNO" typeName="int" />
		<HBaseColumnSchema qualifier="rushSlowNO" typeName="int" />
		<HBaseColumnSchema qualifier="rushTurnNO" typeName="int" />
		<HBaseColumnSchema qualifier="overSpeedNO" typeName="int" />
	</HBaseTableSchema>
	
</SimpleHbase>